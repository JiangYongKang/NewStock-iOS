<!-- 上线的版本号，更新前更改，开发若存在缓存问题手动改变该值 -->
<script>var _sysVersion = "2.0.1";</script>

<!-- 生产版本 -->
<script type="text/javascript" src="../1.2.2/hSea.min.js"></script>
<script type="text/javascript">
    var url = window.location.href ;
    
    if(url.indexOf("?")>-1){
        window.paramObj = {};
        var createParam = function(str){
            var paramArr = str.split("&");
            for(var j=0;j<paramArr.length;j++){
                var oneParam = paramArr[j].split("=");
                if(oneParam.length == 2){
                     paramObj[oneParam[0]] = oneParam[1];
                }
            }
    }
    createParam(window.location.search.substring(1));
}
</script>
